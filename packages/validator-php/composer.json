{
    "name": "form-spec/validator",
    "description": "PHP validator for form-spec system - validates form data against YAML specifications",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Form Spec Team"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FormSpec\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FormSpec\\Validator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
