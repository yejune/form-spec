    {
      "id": "productnft-text-length-001",
      "description": "Text field length validation (minlength/maxlength)",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "name": { "type": "text", "rules": { "required": true, "minlength": 1, "maxlength": 45 } },
              "id": { "type": "text", "rules": { "minlength": 2, "maxlength": 45 } }
            }
          }
        }
      },
      "cases": [
        {
          "input": { "common": { "name": "", "id": "" } },
          "expected": { "valid": false, "error": "required", "field": "common.name" }
        },
        {
          "input": { "common": { "name": "A", "id": "" } },
          "expected": { "valid": true }
        },
        {
          "input": { "common": { "name": "A", "id": "a" } },
          "expected": { "valid": false, "error": "minlength", "field": "common.id" }
        },
        {
          "input": { "common": { "name": "A", "id": "ab" } },
          "expected": { "valid": true }
        },
        {
          "input": { "common": { "name": "This is a very long product name that definitely exceeds forty-five characters limit", "id": "valid-id" } },
          "expected": { "valid": false, "error": "maxlength", "field": "common.name" }
        },
        {
          "input": { "common": { "name": "Valid Product Name", "id": "this-is-a-very-long-product-id-that-definitely-exceeds-forty-five-characters" } },
          "expected": { "valid": false, "error": "maxlength", "field": "common.id" }
        },
        {
          "input": { "common": { "name": "Valid Product Name That Is Exactly Limit", "id": "valid-product-id" } },
          "expected": { "valid": true }
        }
      ]
    },
    {
      "id": "productnft-complex-scenario-001",
      "description": "Complex real-world scenario: Full product with multi-option and quantity tracking",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "name": { "type": "text", "rules": { "required": true, "minlength": 1, "maxlength": 45 } },
              "is_display": { "type": "choice", "default": 1 },
              "display": {
                "type": "group",
                "properties": {
                  "start_dt": { "type": "datetime", "rules": { "required": "..is_display == 2 || ..is_display == 3" } },
                  "end_dt": { "type": "datetime", "rules": { "required": "..is_display == 2" } }
                }
              },
              "is_option": { "type": "choice", "default": 0 },
              "is_quantity": { "type": "choice", "default": 0 },
              "require_point_start": { "type": "number", "default": 0, "rules": { "required": true, "min": 0, "max": 100000000 } },
              "require_point_end": { "type": "number", "default": 0, "rules": { "required": true, "min": 0, "max": 100000000 } },
              "grade_number": { "type": "number", "rules": { "required": true, "min": 1, "max": 10 } },
              "sub_category_seqs": { "type": "select", "multiple": true, "rules": { "required": true, "unique": true } }
            }
          },
          "option": {
            "type": "group",
            "properties": {
              "groups": {
                "type": "group",
                "multiple": true,
                "properties": {
                  "name": { "type": "text", "rules": { "required": "common.is_option == 1" } },
                  "items": { "type": "text", "multiple": true, "rules": { "required": "common.is_option == 1", "unique": true } }
                }
              },
              "button": { "type": "button", "rules": { "required": "common.is_option == 1" } }
            }
          },
          "option_multiplexable": {
            "type": "group",
            "properties": {
              "items": {
                "type": "group",
                "multiple": true,
                "properties": {
                  "name": { "type": "text" },
                  "is_close": { "type": "choice", "default": 0 },
                  "price": { "type": "number", "rules": { "required": "common.is_option == 1 && option_multiplexable.items.*.is_close == 0", "min": 0 } },
                  "total_quantity": { "type": "number", "rules": { "required": "common.is_option == 1 && common.is_quantity > 0 && option_multiplexable.items.*.is_close == 0", "min": 0 } }
                }
              }
            }
          },
          "shipping_methods": { "type": "select", "multiple": true, "rules": { "required": true, "unique": true } }
        }
      },
      "cases": [
        {
          "input": {
            "common": {
              "name": "Premium NFT Collection",
              "is_display": 2,
              "display": { "start_dt": "2024-01-01", "end_dt": "2024-12-31" },
              "is_option": 1,
              "is_quantity": 1,
              "require_point_start": 0,
              "require_point_end": 10000,
              "grade_number": 8,
              "sub_category_seqs": [1, 2]
            },
            "option": {
              "groups": [
                { "name": "Size", "items": ["S", "M", "L", "XL"] },
                { "name": "Color", "items": ["Red", "Blue", "Green"] }
              ],
              "button": "1"
            },
            "option_multiplexable": {
              "items": [
                { "name": "S-Red", "is_close": 0, "price": 50000, "total_quantity": 100 },
                { "name": "S-Blue", "is_close": 0, "price": 50000, "total_quantity": 100 },
                { "name": "M-Red", "is_close": 0, "price": 55000, "total_quantity": 150 },
                { "name": "L-Green", "is_close": 1, "price": null, "total_quantity": null }
              ]
            },
            "shipping_methods": [1, 2]
          },
          "expected": { "valid": true }
        },
        {
          "input": {
            "common": {
              "name": "Premium NFT Collection",
              "is_display": 2,
              "display": { "start_dt": "2024-01-01", "end_dt": "2024-12-31" },
              "is_option": 1,
              "is_quantity": 1,
              "require_point_start": 0,
              "require_point_end": 10000,
              "grade_number": 8,
              "sub_category_seqs": [1, 2]
            },
            "option": {
              "groups": [
                { "name": "Size", "items": ["S", "M", "L"] }
              ],
              "button": "1"
            },
            "option_multiplexable": {
              "items": [
                { "name": "S", "is_close": 0, "price": 50000, "total_quantity": 100 },
                { "name": "M", "is_close": 0, "price": 55000, "total_quantity": null }
              ]
            },
            "shipping_methods": [1]
          },
          "expected": { "valid": false, "error": "required", "field": "option_multiplexable.items.1.total_quantity" }
        }
      ]
    }
  ]
}
