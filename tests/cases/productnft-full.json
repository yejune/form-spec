            }
          },
          "expected": { "valid": true }
        }
      ]
    },
    {
      "id": "productnft-wildcard-array-001",
      "description": "Array wildcard patterns across multiple items",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "is_option": { "type": "choice", "default": 0 },
              "is_quantity": { "type": "choice", "default": 0 }
            }
          },
          "option_single": {
            "type": "group",
            "properties": {
              "items": {
                "type": "group",
                "multiple": true,
                "properties": {
                  "is_close": { "type": "choice", "default": 0 },
                  "original_price": { "type": "number", "default": 0, "rules": { "min": 0 } },
                  "price": { "type": "number", "rules": { "required": "common.is_option == 0 && option_single.items.*.is_close == 0", "min": 0 } },
                  "total_quantity": { "type": "number", "rules": { "required": "common.is_option == 0 && common.is_quantity > 0 && option_single.items.*.is_close == 0", "min": 0 } }
                }
              }
            }
          }
        }
      },
      "cases": [
        {
          "input": {
            "common": { "is_option": 0, "is_quantity": 0 },
            "option_single": { "items": [
              { "is_close": 0, "original_price": 10000, "price": null, "total_quantity": null }
            ]}
          },
          "expected": { "valid": false, "error": "required", "field": "option_single.items.0.price" }
        },
        {
          "input": {
            "common": { "is_option": 0, "is_quantity": 0 },
            "option_single": { "items": [
              { "is_close": 0, "original_price": 10000, "price": 8000, "total_quantity": null },
              { "is_close": 0, "original_price": 12000, "price": null, "total_quantity": null }
            ]}
          },
          "expected": { "valid": false, "error": "required", "field": "option_single.items.1.price" }
        },
        {
          "input": {
            "common": { "is_option": 0, "is_quantity": 1 },
            "option_single": { "items": [
              { "is_close": 0, "original_price": 10000, "price": 8000, "total_quantity": 50 },
              { "is_close": 0, "original_price": 12000, "price": 10000, "total_quantity": null }
            ]}
          },
          "expected": { "valid": false, "error": "required", "field": "option_single.items.1.total_quantity" }
        },
        {
          "input": {
            "common": { "is_option": 0, "is_quantity": 1 },
            "option_single": { "items": [
              { "is_close": 0, "original_price": 10000, "price": 8000, "total_quantity": 50 },
              { "is_close": 1, "original_price": 0, "price": null, "total_quantity": null },
              { "is_close": 0, "original_price": 15000, "price": 12000, "total_quantity": 100 }
            ]}
          },
          "expected": { "valid": true }
        }
      ]
    },
    {
      "id": "productnft-min-validation-001",
      "description": "Min validation for price fields in option items",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "is_option": { "type": "choice", "default": 0 }
            }
          },
          "option_single": {
            "type": "group",
            "properties": {
              "items": {
                "type": "group",
                "multiple": "only",
                "properties": {
                  "is_close": { "type": "choice", "default": 0 },
                  "original_price": { "type": "number", "default": 0, "rules": { "min": 0 } },
                  "price": { "type": "number", "default": 0, "rules": { "required": "common.is_option == 0 && option_single.items.*.is_close == 0", "min": 0 } },
                  "total_quantity": { "type": "number", "rules": { "min": 0 } },
                  "rest_quantity": { "type": "number", "rules": { "min": 0 } }
                }
              }
            }
          }
        }
      },
      "cases": [
        {
          "input": {
            "common": { "is_option": 0 },
            "option_single": { "items": { "is_close": 0, "original_price": -100, "price": 5000, "total_quantity": 10, "rest_quantity": 10 } }
          },
          "expected": { "valid": false, "error": "min", "field": "option_single.items.original_price" }
        },
        {
          "input": {
            "common": { "is_option": 0 },
            "option_single": { "items": { "is_close": 0, "original_price": 10000, "price": -50, "total_quantity": 10, "rest_quantity": 10 } }
          },
          "expected": { "valid": false, "error": "min", "field": "option_single.items.price" }
        },
        {
          "input": {
            "common": { "is_option": 0 },
            "option_single": { "items": { "is_close": 0, "original_price": 10000, "price": 8000, "total_quantity": -5, "rest_quantity": 10 } }
          },
          "expected": { "valid": false, "error": "min", "field": "option_single.items.total_quantity" }
        },
        {
          "input": {
            "common": { "is_option": 0 },
            "option_single": { "items": { "is_close": 0, "original_price": 10000, "price": 8000, "total_quantity": 10, "rest_quantity": -1 } }
          },
          "expected": { "valid": false, "error": "min", "field": "option_single.items.rest_quantity" }
        },
        {
          "input": {
            "common": { "is_option": 0 },
            "option_single": { "items": { "is_close": 0, "original_price": 0, "price": 0, "total_quantity": 0, "rest_quantity": 0 } }
          },
          "expected": { "valid": true }
        }
      ]
    },
    {
      "id": "productnft-unique-validation-001",
      "description": "Unique validation for multiple select fields",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "category_seqs": { "type": "select", "multiple": true, "rules": { "unique": true } },
              "sub_category_seqs": { "type": "select", "multiple": true, "rules": { "required": true, "unique": true } }
            }
          },
          "additionals": { "type": "select", "multiple": true, "rules": { "unique": true } },
          "shipping_methods": { "type": "select", "multiple": true, "rules": { "required": true, "unique": true } }
        }
      },
      "cases": [
        {
          "input": {
            "common": { "category_seqs": [1, 2, 3], "sub_category_seqs": [10, 20, 30] },
            "additionals": [100, 200],
            "shipping_methods": [1, 2]
          },
          "expected": { "valid": true }
        },
        {
          "input": {
            "common": { "category_seqs": [1, 2, 1], "sub_category_seqs": [10, 20, 30] },
            "additionals": [],
            "shipping_methods": [1]
          },
          "expected": { "valid": false, "error": "unique", "field": "common.category_seqs" }
        },
        {
          "input": {
            "common": { "category_seqs": [1, 2, 3], "sub_category_seqs": [10, 20, 10] },
            "additionals": [],
            "shipping_methods": [1]
          },
          "expected": { "valid": false, "error": "unique", "field": "common.sub_category_seqs" }
        },
        {
          "input": {
            "common": { "category_seqs": [], "sub_category_seqs": [10] },
            "additionals": [100, 100, 200],
            "shipping_methods": [1]
          },
          "expected": { "valid": false, "error": "unique", "field": "additionals" }
        },
        {
          "input": {
            "common": { "category_seqs": [], "sub_category_seqs": [10] },
            "additionals": [],
            "shipping_methods": [1, 2, 1]
          },
          "expected": { "valid": false, "error": "unique", "field": "shipping_methods" }
        },
        {
          "input": {
            "common": { "category_seqs": [], "sub_category_seqs": [] },
            "additionals": [],
            "shipping_methods": [1]
          },
          "expected": { "valid": false, "error": "required", "field": "common.sub_category_seqs" }
        },
        {
          "input": {
            "common": { "category_seqs": [], "sub_category_seqs": [10, 20] },
            "additionals": [],
            "shipping_methods": []
          },
          "expected": { "valid": false, "error": "required", "field": "shipping_methods" }
        }
      ]
    },
    {
      "id": "productnft-grade-points-001",
      "description": "Grade and points validation with min/max",
      "spec": {
        "type": "group",
        "properties": {
          "common": {
            "type": "group",
            "properties": {
              "require_point_start": { "type": "number", "default": 0, "rules": { "required": true, "min": 0, "max": 100000000 } },
              "require_point_end": { "type": "number", "default": 0, "rules": { "required": true, "min": 0, "max": 100000000 } },
              "grade_number": { "type": "number", "rules": { "required": true, "min": 1, "max": 10 } }
            }
          }
        }
      },
      "cases": [
        {
          "input": { "common": { "require_point_start": null, "require_point_end": 1000, "grade_number": 5 } },
          "expected": { "valid": false, "error": "required", "field": "common.require_point_start" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": null, "grade_number": 5 } },
          "expected": { "valid": false, "error": "required", "field": "common.require_point_end" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": 1000, "grade_number": null } },
          "expected": { "valid": false, "error": "required", "field": "common.grade_number" }
        },
        {
          "input": { "common": { "require_point_start": -1, "require_point_end": 1000, "grade_number": 5 } },
          "expected": { "valid": false, "error": "min", "field": "common.require_point_start" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": -1, "grade_number": 5 } },
          "expected": { "valid": false, "error": "min", "field": "common.require_point_end" }
        },
        {
          "input": { "common": { "require_point_start": 100000001, "require_point_end": 1000, "grade_number": 5 } },
          "expected": { "valid": false, "error": "max", "field": "common.require_point_start" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": 100000001, "grade_number": 5 } },
          "expected": { "valid": false, "error": "max", "field": "common.require_point_end" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": 1000, "grade_number": 0 } },
          "expected": { "valid": false, "error": "min", "field": "common.grade_number" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": 1000, "grade_number": 11 } },
          "expected": { "valid": false, "error": "max", "field": "common.grade_number" }
        },
        {
          "input": { "common": { "require_point_start": 0, "require_point_end": 100000000, "grade_number": 10 } },
          "expected": { "valid": true }
        },
        {
          "input": { "common": { "require_point_start": 1000, "require_point_end": 5000, "grade_number": 1 } },
          "expected": { "valid": true }
        }
      ]
